<Window x:Class="Wpf_SmallWorld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="900"
        MinHeight="600" MinWidth="900"
        xmlns:local="clr-namespace:Wpf_SmallWorld"
        WindowStartupLocation="CenterScreen"
        Icon="./Resources/smallWorld.png">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
        </Grid.RowDefinitions>
        
        <Grid Grid.ColumnSpan="2">
                <Frame Name="PagePrincipale" Source="PagePrincipale.xaml" NavigationUIVisibility="Hidden"/>
        </Grid>
 
        <Grid Grid.Column="1" Name="UserControl">
            <Frame Name="PageSelection" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
   

</Window>
